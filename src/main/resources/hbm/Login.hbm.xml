<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true">
   <class name="com.genie.entity.Login" table="t_login" mutable="true" dynamic-update="false" dynamic-insert="false" select-before-update="false" lazy="true" abstract="false">
      <id name="id">
         <column name="id" sql-type="int" not-null="true"/>
      </id>
      <property name="username" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="username" sql-type="varchar(254)" length="254"/>
      </property>
      <property name="password" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="password" sql-type="varchar(254)" length="254"/>
      </property>
      <bag name="role" table="t_login_role" outer-join="false" lazy="true" optimistic-lock="true">
      	<key>
      		<column name="id" sql-type="int" not-null="true" length="0"/>
      	</key>
      	<many-to-many class="com.genie.entity.Role" outer-join="false">
      		<column name="rol_id" sql-type="int" not-null="true" length="0"/>
      	</many-to-many>
      </bag>
   </class>
</hibernate-mapping>