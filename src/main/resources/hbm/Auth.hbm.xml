<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true">
   <class name="com.genie.entity.Auth" table="t_auth" mutable="true" dynamic-update="false" dynamic-insert="false" select-before-update="false" lazy="true" abstract="false">
      <id name="id">
         <column name="id" sql-type="int" not-null="true"/>
      </id>
      <property name="authName" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="authName" sql-type="varchar(254)" length="254"/>
      </property>
      <property name="authPath" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="authPath" sql-type="varchar(254)" length="254"/>
      </property>
      <property name="description" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="description" sql-type="varchar(254)" length="254"/>
      </property>
      <bag name="role" table="t_role_auth" outer-join="false" inverse="true" lazy="true" optimistic-lock="true">
      	<key>
      		<column name="role_id" sql-type="int" not-null="true" length="0"/>
      	</key>
      	<many-to-many class="com.genie.entity.Role" outer-join="false">
      		<column name="id" sql-type="int" not-null="true" length="0"/>
      	</many-to-many>
      </bag>
   </class>
</hibernate-mapping>